{
    "extends":["airbnb"],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "linebreak-style":"off",
        "max-len":["error",300],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/button-has-type": [1, {
            "button": true,
            "submit": true,
            "reset": true
          }],
          "react/prop-types":["off"] ,
          "react/destructuring-assignment": [0, "never"]
      }
}